{
	"name": "jaakon/mopsis",
	"type": "library",
	"description": "The Mopsis Framework.",
	"keywords": ["framework", "mopsis"],
	"homepage": "https://github.com/jaakon/mopsis",
	"license": "MIT",
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/jaakon/PHP-DI"
		}
	],
	"require": {
		"php":                         ">=7.0",
		"aura/accept":                 "^2.2",
		"aura/filter":                 "^2.0",
		"aura/web":                    "^2.0",
		"codezero/cookie":             "^1.0",
		"filp/whoops":                 "^2.0",
		"fluentdom/fluentdom":         "^5.3",
		"FluentDOM/Selectors-Symfony": "^1.0",
		"illuminate/config":           "^5.1",
		"illuminate/database":         "^5.1",
		"illuminate/events":           "^5.1",
		"illuminate/translation":      "^5.1",
		"league/flysystem":            "^1.0",
		"monolog/monolog":             "^1.16",
		"patchwork/utf8":              "^1.2",
		"php-di/php-di":               "dev-dot-helper as 5.4.0",
		"raulfraile/ladybug":          "^1.0",
		"swiftmailer/swiftmailer":     "^5.4",
		"symfony/var-dumper":          "^3.1",
		"tedivm/stash":                "^0.14",
		"twig/twig":                   "^1.20"
	},
	"suggest": {
		"aptoma/twig-markdown":        "Twig extension to work with Markdown content",
		"asm89/twig-cache-extension":  "Cache fragments of templates directly within Twig",
		"erusev/parsedown":            "Parser for Markdown",
		"gordalina/cachetool":         "Manage your opcode & apc through the cli",
		"jschaedl/iban":               "validate and generate IBANs"
    },
	"autoload": {
		"psr-4": {
			"Mopsis\\": "src"
		}
	},
	"bin": [
		"bin/mopsis"
	]
}
